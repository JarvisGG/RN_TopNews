// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "editor.fontSize": 12,
    "files.autoSave": "afterDelay",
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "editor.insertSpaces": true,
    "editor.lineNumbers": "on",
    "editor.acceptSuggestionOnEnter": true
}